{
  "name": "Timer",
  "description": "A simple timer",
  "version": "1.9",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting", "background", "tabs", "windows"],
  "background": {
    "service_worker": "background.js"
  },  
  "action": {
    "default_popup": "index.html",
    "default_icon": "Assets/icon.png"
  }
}